<page xmlns:x="http://www.yandex.ru/xscript">
    <xscript>
        <add-headers>
            <header name="Cache-Control" value="max-age=0, proxy-revalidate"/>
            <header name="Pragma" value="no-cache"/>
        </add-headers>
    </xscript>

    <x:control>
        <method>reset-response-time</method>
    </x:control>
</page>
